#!/usr/bin/env ruby
here = File.dirname(__FILE__)
$LOAD_PATH << File.expand_path(File.join(here, '..', 'lib'))

require 'docker_echo'
require 'yaml'

se = ServerEngine.create(nil, DockerEcho::Serverengine, {
  log:         'myserver.log',
  pid_path:    'myserver.pid',
  worker_type: 'process',
  workers:     2,
})
se.run
